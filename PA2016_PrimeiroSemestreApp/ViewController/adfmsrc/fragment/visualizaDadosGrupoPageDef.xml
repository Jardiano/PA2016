      <graphDataMap convert="false" leafOnly="true">
        <series>
          <data>
            <item value="RegressaoValorCliMes" label="Regressão Cliente"/>
            <item value="RegressaoValorFatMes" label="Regressão Faturado"/>
          </data>
        </series>
        <groups>
          <item value="MesNum" label="MesDesc"/>
        </groups>
      </graphDataMap>
    </graph>
  </bindings>
