        <series>
          <data>
            <item value="CenariopessimistaFat" label="Cenario Pessimista"/>
            <item value="CenariootimistaFat" label="Cenario Otimista"/>
            <item value="CenarioprovavelFat" label="Cenario Provavel"/>
          </data>
        </series>
        <groups>
          <item value="MesNum" label="MesDesc"/>
        </groups>
      </graphDataMap>
